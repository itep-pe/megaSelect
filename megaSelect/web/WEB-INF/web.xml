<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>SelectController</servlet-name>
        <servlet-class>ajax.SelectController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MegaSelect</servlet-name>
        <servlet-class>ajax.MegaSelect</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SelectController</servlet-name>
        <url-pattern>/selectController</url-pattern>
    </servlet-mapping>
    <error-page>
        <error-code>404</error-code>
        <location>/erro/erro.jsp</location>
    </error-page>
    <servlet-mapping>
        <servlet-name>MegaSelect</servlet-name>
        <url-pattern>/megaSelect</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
